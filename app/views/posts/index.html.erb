<h1> Global Feed </h1>
 <% @posts.each do |post|%>
 <div class="row small-up-1 medium-up-2 large-up-3">
   <div class="column">
     <img src=" <%= post.image %>" class="thumbnail" alt="">
     <h4 class="text-center"> <%= post.title %> </h4>
     <p class="body-text"> <%= post.text %></p>
     <p>
       <%= post.user_id %>
     </p>
   </div>
 </div>
 <% end %>
