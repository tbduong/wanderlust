<br>
<div class="large-6 large-centered text-center">
  <div class="callout form-box">
    <h1 class="form-title">Login</h1>
    <%= form_tag '/login' do %>
    Email: <%= text_field_tag :email %>
    Password: <%= password_field_tag :password %>
    <%= submit_tag "Submit", class:"button" %>
  <% end %>
  <hr>
  <p><%= link_to "Don't have an account yet? Sign Up for free here!", signup_path %>
  </p>
    </div>
</div>
